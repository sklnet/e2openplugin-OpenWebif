	 \$("#accordion").accordion({
                active:false,
                change:function(event, ui) {
                        ui.oldContent.empty();
                        ui.newContent.load(ui.newHeader.find('a').attr('id'));
                },
                autoHeight: false,
		 collapsible: true
            });


</script>

<div id="accordion">

#for $bouquet in $bouquets
